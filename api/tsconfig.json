{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"verbatimModuleSyntax": true,
		"strict": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"baseUrl": "./",
		"paths": {
			"@/*": [
				"./src/*"
			]
		},
		"outDir": "./dist",
		"types": [
			"bun-types"
		],
		"composite": true,
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	},
	"include": [
		"src/**/*",
		"prisma/seed/json/games2.json",
		"prisma/generated/**/*"
	],
	"tsc-alias": {
		"resolveFullPaths": true
	}
}